package ch03.sec01;

public class BreakOutterExample {
	public static void main(String[] args) {
		int sum = 0;
		Outter: for(char upper='A'; upper<='Z'; upper++) {
			for(char lower='a'; lower<='z'; lower++) {
				if (lower == 'g') {
					break Outter;
				}
			}
		}
		System.out.println(sum);
	}
}

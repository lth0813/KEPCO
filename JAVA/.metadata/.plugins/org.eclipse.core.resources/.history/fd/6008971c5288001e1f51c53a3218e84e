package ch03.sec01;

import java.util.Scanner;
public class KeyControlExample {
	public static void main (String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("====================");
		System.out.println("1.증가 | 2.감속 | 3.중지");
		System.out.println("====================");
		System.out.print("선택");
		String val = scanner.nextLine();
	}
}
